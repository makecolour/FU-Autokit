{
	"manifest_version": 3,
	"name": "FU Autokit",
	"version": "6.9",
	"description": "Em yêu 4 bản",
	"action": {
		"default_popup": "./popup/popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://fap.fpt.edu.vn/Default.aspx/*",
				"https://fap.fpt.edu.vn/"
			],
			"js": [
				"./content/fap_login.js"
			]
			, "world": "MAIN"
		},
		{
			"matches": [
				"https://cmshn.fpt.edu.vn/*",
				"https://cmshn.fpt.edu.vn/login/index.php"
			],
			"js": [
				"./content/cms_login.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://dng.fpt.edu.vn/*"
			],
			"js": [
				"./utils/storage.js",
				"./content/dng_login.js"
			]
		},
		{
			"matches": [
				"https://accounts.google.com/v3/signin/identifier?*fap.fpt.edu.vn*",
				"https://accounts.google.com/v3/signin/identifier?*cmshn.fpt.edu.vn*",
				"https://accounts.google.com/v3/signin/identifier?*fu-edunext.fpt.edu.vn*",
				"https://accounts.google.com/v3/signin/identifier?*flm.fpt.edu.vn*"
			],
			"js": [
				"./utils/storage.js",
				"./content/auto_login.js"
			]
		},
		{
			"matches": [
				"https://flm.fpt.edu.vn/",
				"https://flm.fpt.edu.vn/DefaultSignin"
			],
			"js": [
				"./content/flm_login.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://flm.fpt.edu.vn/*"
			],
			"js": [
				"./content/flm_setlang.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://flm.fpt.edu.vn/gui/role/student/ListCurriculum",
				"https://flm.fpt.edu.vn/gui/role/student/SyllabusManagement",
				"https://flm.fpt.edu.vn/gui/tool/AllPrequisiteSubject",
				"https://flm.fpt.edu.vn/gui/tool/AllCorollarySubject"
			],
			"js": [
				"./content/flm_autocomplete.js"
			],
			"css":[
				"./utils/css/autocomplete.css"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://library.fpt.edu.vn/"
			],
			"js": [
				"./content/library_login.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://library.fpt.edu.vn/Auth/Login"
			],
			"js": [
				"./utils/storage.js",
				"./content/process_library.js"
			]
		},
		{
			"matches": [
				"https://accounts.google.com/o/oauth2/auth*fap.fpt.edu.vn*",
				"https://accounts.google.com/o/oauth2/v2/auth*cmshn.fpt.edu.vn*"
			],
			"js": [
				"./utils/storage.js",
				"./content/google.js"
			]
		},
		{
			"matches": [
				"https://accounts.google.com/gsi/select?*fu-edunext.fpt.edu.vn*",
				"https://accounts.google.com/gsi/select?*flm.fpt.edu.vn*"
			],
			"js": [
				"./utils/storage.js",
				"./content/gsi.js"
			]
		},
		{
			"matches": [
				"https://fap.fpt.edu.vn/Feedback/*"
			],
			"js": [
				"./content/feedback.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://fu-edunext.fpt.edu.vn/"
			],
			"js": [
				"./content/edunext_redirect.js"
			],
			"world": "MAIN"
		},
		{
			"matches": ["https://fu-edunext.fpt.edu.vn/course/activity/*"],
			"js": ["/content/edunext_grading.js"]
		},
		{
			"matches": [
				"https://tinder.com/*"
			],
			"js": [
				"./content/web_t.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://fu-edunext.fpt.edu.vn/login"
			],
			"js": [
				"./content/edunext_process.js"
			]
			,"world" :"MAIN"
		}
		,{
			"matches": [
			  "https://fap.fpt.edu.vn/Grade/StudentTranscript.aspx",
			  "http://fap.fpt.edu.vn/Grade/StudentTranscript.aspx"
			],
			"css": [
			  "./utils/css/gpa.css"
			],
			"js": [
			  "content/gpa_logic.js"
			],
			"run_at": "document_end"
		  }
	],

	"permissions": [
		"storage"
	],
	"icons": {
		"16": "./assets/LogoChill.png",
		"32": "./assets/LogoChill.png",
		"48": "./assets/LogoChill.png",
		"128": "./assets/LogoChill.png"
	}
}